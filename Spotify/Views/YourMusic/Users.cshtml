@model IEnumerable<Spotify.Models.User>
@{
    ViewBag.Title = "Following Users";
}
<!--Gives the menu-->
@Html.Partial("_YourMusic")
<h2>Playlists</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
